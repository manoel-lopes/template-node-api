{
  "extends": "@rocketseat/eslint-config/node",
  "rules": {
    "no-useless-constructor": "off",
    "no-redeclare": "off",
    "no-var": "error",
    "@typescript-eslint/parameter-properties": [
      "error",
      {
        "allow": ["readonly", "private readonly"]
      }
    ],
    "@typescript-eslint/no-explicit-any": "error"
  }
}
